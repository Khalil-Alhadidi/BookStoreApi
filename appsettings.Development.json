{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "Jwt": {
    "Key": "ThisIsASecretKeyForJwt;This Should be a very long password for the SHA512",
    "Issuer": "IssuerPlaceHolder",
    "Audience": "AudiencePlaceHolder"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=IP,Port;Database=YourDbName;User Id=YourDbUserName;Password=PasswordGoesHere;TrustServerCertificate=True;"
  },
  "Security": {
    "PasswordSalt": "PasswordSaltPlaceHolder"
  },
  "Smtp": {
    "Server": "smtp.example.com",
    "Port": 587,
    "Username": "your-email@example.com",
    "Password": "your-password"
  },
  "FrontendUrl": "URL goes here!"
}
